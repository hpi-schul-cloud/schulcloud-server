		"@feathersjs/express": "^4.5.4",
		"@feathersjs/feathers": "^4.5.3",
		"@feathersjs/socketio": "^4.5.4",
		"@schul-cloud/commons": "^1.0.20",
		"@sentry/node": "^5.9.0",
		"ajv": "^6.10.2",
		"amqplib": "^0.5.5",
		"arg": "^4.1.3",
