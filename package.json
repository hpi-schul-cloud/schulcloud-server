{
	"name": "schulcloud-server",
	"description": "dBildungscloud server",
	"homepage": "https://dBildungscloud.de/",
	"main": "src/",
	"keywords": [
		"feathers",
		"nest",
		"jest",
		"domain driven design"
	],
	"license": "AGPL-3.0",
