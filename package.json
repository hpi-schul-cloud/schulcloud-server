  },
  "devDependencies": {
    "codecov": "^3.5.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^6.1.4",
    "mockery": "^2.0.0",
    "nodemailer-mock": "^1.1.10",
    "nyc": "^14.1.1",
