{
	"monorepo": true,
	"root": "apps/server",
	"sourceRoot": "apps/server/src",
	"compilerOptions": {
		"webpack": true,
		"tsConfigPath": "apps/server/tsconfig.app.json"
	},
	"projects": {
		"server": {
			"type": "application",
			"root": "apps/server",
			"entryFile": "main",
			"sourceRoot": "apps/server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/server/tsconfig.app.json"
			}
		},
		"legacy-server": {
			"type": "application",
			"root": "apps/legacy-server",
			"entryFile": "src/index",
			"sourceRoot": "apps/legacy-server/src",
			"compilerOptions": {
				"tsConfigPath": "apps/legacy-server/tsconfig.app.json"
			}
		}
	}
}
