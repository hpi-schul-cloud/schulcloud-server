apiVersion: v1
kind: ConfigMap
metadata:
  namespace: {{ NAMESPACE }}
  name: schulconnex-group-removal-configmap
  labels:
    app: schulconnex-group-removal
data:
  NODE_OPTIONS: "--max-old-space-size=1536"
  EXIT_ON_ERROR: "true"
  NEST_LOG_LEVEL: "{{ NEST_LOG_LEVEL }}"
  FEATURE_SCHULCONNEX_COURSE_SYNC_ENABLED: "{{ FEATURE_SCHULCONNEX_COURSE_SYNC_ENABLED }}"
  SCHULCONNEX_PROVISIONING_AMQP__GROUP_REMOVAL_PREFETCH_COUNT: "{{ SCHULCONNEX_PROVISIONING_AMQP__GROUP_REMOVAL_PREFETCH_COUNT }}"
