apiVersion: v1
kind: Service
metadata:
  name: api-fwu-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: api-fwu
  annotations:
    prometheus.io/scrape: 'true'
    prometheus.io/port: '4446'
spec:
  type: ClusterIP
  ports:
    - port: 4446
      targetPort: 4446
      protocol: TCP
      name: api-fwu
  selector:
    app: api-fwu
