apiVersion: v1
kind: Service
metadata:
  name: tldraw-server-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: tldraw
spec:
  type: ClusterIP
  ports:
    - port: 3345
      targetPort: 3345
      protocol: TCP
      name: tldraw-ws-server
    - port: 3349
      targetPort: 3349
      protocol: TCP
      name: tldraw-http-management
  selector:
    app: tldraw
