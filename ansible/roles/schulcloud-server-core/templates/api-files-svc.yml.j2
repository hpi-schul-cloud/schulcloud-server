apiVersion: v1
kind: Service
metadata:
  name: api-files-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: api-files
  annotations:
    prometheus.io/scrape: 'true'
    prometheus.io/port: '4444'
spec:
  type: ClusterIP
  ports:
    - port: 4444
      targetPort: 4444
      protocol: TCP
      name: api-files
  selector:
    app: api-files
