apiVersion: v1
kind: Service
metadata:
  name: mgmt-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: management-deployment
  annotations:
    prometheus.io/scrape: 'true'
    prometheus.io/port: '3333'
spec:
  type: ClusterIP
  ports:
    - port: 3333
      targetPort: 3333
      protocol: TCP
      name: mgmt-api
  selector:
    app: management-deployment
